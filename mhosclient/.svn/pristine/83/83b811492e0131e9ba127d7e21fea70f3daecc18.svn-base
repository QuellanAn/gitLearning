		<action name="paymentStatisticsAction_*" class="com.catic.mobilehos.action.PaymentStatisticsAction"
			method="{1}">
		</action>
		
		<!-- 服务评价统计 -->
		<action name="serviceEvaluationStatisticsAction_*" class="com.catic.mobilehos.action.ServiceEvaluationStatisticsAction"
			method="{1}">
