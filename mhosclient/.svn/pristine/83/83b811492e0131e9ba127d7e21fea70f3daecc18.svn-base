			<result name="singleDepartmentCount">/statistics/serviceEvaluation/serviceEvaluation_department_single_main.jsp</result>
			<result name="doctorCount">/statistics/serviceEvaluation/serviceEvaluation_doctor_main.jsp</result>
		</action>
	</package>

