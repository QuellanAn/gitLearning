<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"
	import="java.util.*, com.catic.mobilehos.service.vo.*"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

<head>
<base href="<%=basePath%>/" />
<title>对账操作记录</title>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<link rel="stylesheet" href="resources/css/smoothness/jquery-ui.css" />
<link rel="stylesheet" type="text/css" href="resources/css/global.css" />
<link rel="stylesheet" type="text/css" href="resources/css/common.css" />
<link rel="stylesheet" type="text/css" href="resources/css/index.css" />
<script charset="utf-8" src="resources/js/jquery/jquery-1.10.2.js"></script>
<script type="text/javascript" src="resources/js/jquery/jquery-ui.js"></script>
<script type="text/javascript" src="resources/js/common.js"></script>
<script type="text/javascript"
	src="resources/js/jquery/jquery.ui.datepicker-zh-CN.js"></script>
<script type="text/javascript" src="../getKey.js"></script>
<script charset="utf-8" src="checkRecord/checkRecord_main.js"></script>
</head>
<!-- <s:if test="#session.userName==null">
	<script type="text/javascript">
		location.href = $('base').attr('href') + "login.jsp";
	</script>
</s:if> -->
<input type="hidden" value="${param.key}" id="key1"/>
<s:hidden value="%{#session.key}" id="key"/>
<body>
	<div class="cnt-wrap">
		<s:include value="../menus.jsp"></s:include>
		<div class="right-cnt-wrap f-fl">
			<ul class="tab-wrap bg">
				<li><a href="../check/check_main.jsp">多方对账</a></li>
				<li class="tab-crt">对账操作记录</li>
			</ul>
			<div class="tab-cnt">
				<div class="f-fl left-list-wrap">
					<div class="left-list-hd">
						<span class="f-fl">对账操作记录(<span id="totalcount"></span>) </span>
					</div>
					<div class="left-list-hd" style="height: 130px; margin:0px;">
					<form id="form_query"  onsubmit="return false;">
						<table height="100%" style="margin-top: -3px;">
							<tr>
								<td valign="middle" >账单类型:</td>
								<td valign="middle">
									<input type="radio" name="recordType" checked="checked" id="bill" value="0" />日结账单 
								</td>
								<td valign="middle">
									<input type="radio" id="order" name="recordType" value="1" />普通订单
								</td>
							</tr>
							<!-- <tr>
								<td valign="middle" >订单类型:</td>
								<td valign="middle"><input type="radio" name="recordType" checked="checked" id="bill" value="0" />交易 
								</td>
								<td valign="middle">
									<input type="radio" id="order" name="recordType" value="1" />退款
								</td>
							</tr> -->
							<!-- <tr id="billTr">
								<td valign="middle" align="right">交易更新时间:</td>
								<td valign="middle" align="left"><input name="startdate1"
									type="text" size="17" style="height: 28px; line-height: 28px;" />&nbsp;-&nbsp;
								</td>
								<td valign="middle" align="left"><input name="enddate1"
									type="text" size="17" style="height: 28px; line-height: 28px;margin-top: 4px;" />
								</td>
								<td valign="msiddle" style="padding-left: 15px;"><button id="act_querybill1"
										class="save-btn">查询</button>
								</td>
							<tr> -->
							<tr id="orderTr">
								<td valign="middle" align="left">交易单号:</td>
								<td valign="middle">
								<input type="text"  size="16" name="transactionNum"  id="transactionNum" style="height: 28px; line-height: 28px; ">
								</td>
								<td valign="middle" align="left">第三方交易流水号:</td>
								<td valign="middle">
								<input type="text" id="orderCode" name="orderCode" size="16"  style="height: 28px; line-height: 28px;">
								</td>
								<td valign="middle" align="right">HIS交易流水号:</td>
								<td valign="middle">
								<input type="text"  size="16" name="hisTransId" id="hisTransId" style="height: 28px; line-height: 28px;">
								</td>
							<tr>
							<tr id="orderbill">
								<td valign="middle" align="right">交易更新时间:</td>
								<td valign="middle"><input name="startdate"
									type="text" size="16" style="height: 28px; line-height: 28px;" />&nbsp;-&nbsp;
								</td>
								<td valign="middle"><input name="enddate"
									type="text" size="16" style="height: 28px; line-height: 28px;margin-top: 4px;padding-left:-20px;" />
								</td>
								<td valign="msiddle" style="padding-left: 15px;"><button id="act_querybill"
										class="save-btn">查询</button>
								</td>
							<tr>
						</table>
						</form>
					</div>
					<div id="div_cnt_table"></div>
				</div>
				<div class="f-clear"></div>
			</div>
		</div>

		<div class="f-clear"></div>
	</div>
	<div id="showPayInfo"></div>
	<div id="showPatients"></div>
</body>
</script>
</html>