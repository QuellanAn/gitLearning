package com.catic.mobilehos;

/**
 * 接口错误代码定义
 * @author xieweipeng
 *
 */
public interface ErrorCode {
	
	String ERROR_CODE = "errorcode";
	
	String ERROR_TARGET = "target";
	
	String MSG = "msg";
	
	// 无效的签名, 客户端生成的签名和服务端的不匹配
	String LS_ILLEGAL_SIGN = "LS_ILLEGAL_SIGN";
	
	// 提交的参数无效，非法，或者不符合业务规则
	String LS_ILLEGAL_BUS_ARGUMENT = "LS_ILLEGAL_BUS_ARGUMENT";
	
	// 平台服务器处理业务时出现异常
	String LS_BUS_EXEC_ERR = "LS_BUS_EXEC_ERR";
	
	// 平台服务器访问预约挂号系统出错，如：无法连接预约挂号系统
	String RS_CALL_REGSYS__ERR = "RS_CALL_REGSYS__ERR";
	
	// 请求在预约挂号系统执行错误，可能参数验证失败，或者执行出现异常
	String RS_REGSYS_EXEC_ERR = "RS_REGSYS_EXEC_ERR";
	
	  /** 请求参数无效 */
    String LS_ILLEGAL_ARGUMENT = "LS_ILLEGAL_ARGUMENT";

}
